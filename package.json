{
  "name": "proyecto-final-coderhouse",
  "version": "1.0.0",
  "description": "Proyecto Final Coderhouse Backend - eCommerce",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon -L -r tsconfig-paths/register ./src/index.ts",
    "start": "npx tsc && tspath -f && node ./build/src/index.js",
    "build": "npx tsc",
    "lint": "eslint . --ext .ts",
    "prettier": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "migrate:db": "npx knex migrate:latest --env development",
    "rollback:db": "npx knex migrate:rollback --env development",
    "seed:db": "npx knex seed:run --env development",
    "init:db": "npm run rollback:db && npm run migrate:db && npm run seed:db",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Lucas Caniggia",
  "license": "ISC",
  "dependencies": {
    "@types/faker": "^5.5.8",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "express-session": "^1.17.2",
    "faker": "^5.5.3",
    "firebase-admin": "^9.11.1",
    "knex": "^0.95.10",
    "moment": "^2.29.1",
    "mongoose": "^6.0.5",
    "mysql": "^2.18.1",
    "normalizr": "^3.6.1",
    "socket.io": "^4.1.3",
    "sqlite3": "^5.0.2",
    "tsconfig-paths": "^3.11.0",
    "tspath": "^1.3.7",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/node": "^16.7.1",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.1",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "ts-node": "^10.2.1",
    "typescript": "^4.3.5"
  }
}
